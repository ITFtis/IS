<script src="~/Scripts/prj/consult/base.js"></script>
<script src="~/Scripts/gis/bootstraptable/extensions/export/bootstrap-table-export.js"></script>
<script src="https://cdn.jsdelivr.net/npm/tableexport.jquery.plugin@1.10.21/tableExport.min.js"></script>
<script src="~/Scripts/gis/other/xlsx.full.min.js"></script>


@using Dou.Misc.Extension
@Html.MenuMap()
@Html.DataManagerOptionsJavascript(null, "dou_options")
<style>
    .table-cinfo > tbody > tr > td:first-child {
        font-weight: bold;
    }

    .reply-f-title {
        margin-right: .5rem;
        line-height: 1.3;
    }
</style>
<script>
    $(document).ready(function () {
        $('.export').off('click').on('click', function () {
            $('.consult-table').DouEditableTable('showTableColumn', 'ReplyLogs', true);
            window.helper.misc.tableToExcel($('.consult-table'), "Table1", "myfile");
            $('.consult-table').DouEditableTable('showTableColumn', 'ReplyLogs', false);
        })
    })
</script>